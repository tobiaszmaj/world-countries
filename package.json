{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "framer-motion": "^2.1.2",
    "gatsby": "^2.24.10",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-layout": "^1.3.10",
    "gatsby-plugin-manifest": "^2.4.20",
    "gatsby-plugin-offline": "^3.2.20",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.21",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-source-apiserver": "^2.1.8",
    "gatsby-source-filesystem": "^2.3.22",
    "gatsby-transformer-sharp": "^2.5.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-infinite-scroll-component": "^5.0.5",
    "react-loading-skeleton": "^2.1.1",
    "react-scroll": "^1.8.0",
    "slugify": "^1.4.4",
    "styled-components": "^4.4.1",
    "styled-reset": "^4.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-preset-gatsby": "^0.5.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5"
  },
  "resolutions": {
    "babel-plugin-remove-graphql-queries": "2.9.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --config .prettierrc --write",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}